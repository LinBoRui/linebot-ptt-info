digraph "State Machine" {
	graph [color=black directed=true fillcolor=white label="State Machine" rankdir=LR "strict"=false style=solid]
	node [color=black fillcolor=white peripheries=1 shape=rectangle style="rounded, filled"]
	edge [color=black]
	init [label="init\l"]
	hot_articles [label="hot_articles\l"]
	hot_boards [label="hot_boards\l"]
	articles [label="articles\l"]
	board_articles [label="board_articles\l"]
	links [label="links\l" color=red fillcolor=darksalmon peripheries=2]
	init -> hot_articles [label="advance [is_going_to_hot_articles]"]
	init -> hot_boards [label="advance [is_going_to_hot_boards]"]
	init -> articles [label="advance [is_going_to_articles]"]
	init -> init [label="advance [is_going_to_init]"]
	hot_boards -> hot_articles [label="advance [is_going_to_hot_articles]"]
	hot_boards -> board_articles [label="advance [is_going_to_board_articles]"]
	hot_boards -> init [label="advance [is_going_to_init]"]
	articles -> hot_articles [label="advance [is_going_to_hot_articles]"]
	articles -> hot_boards [label="advance [is_going_to_hot_boards]"]
	articles -> links [label="advance [is_going_to_links]"]
	articles -> init [label="advance [is_going_to_init]"]
	board_articles -> hot_articles [label="advance [is_going_to_hot_articles]"]
	board_articles -> hot_boards [label="advance [is_going_to_hot_boards]"]
	board_articles -> links [label="advance [is_going_to_links]"]
	board_articles -> init [label="advance [is_going_to_init]"]
	hot_articles -> hot_boards [label="advance [is_going_to_hot_boards]"]
	hot_articles -> articles [label="advance [is_going_to_articles]"]
	hot_articles -> init [label="advance [is_going_to_init]"]
	links -> links [label="advance [is_going_to_links]" color=blue]
	links -> init [label="advance [is_going_to_init]"]
}
